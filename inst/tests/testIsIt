  expect_that(isit("+", list(3,4), 7), equals(TRUE))
  expect_that(isit("-", list(3,4), -1), equals(TRUE))
  expect_that(isit("*", list(3,4), 12), equals(TRUE))
  expect_that(isit("/", list(12,4), 3), equals(TRUE))
  expect_that(isit("%%", list(11,4), 3), equals(TRUE))
  expect_that(isit("%/%", list(11,4), 2), equals(TRUE))
  expect_that(isit("<", list(3,4), TRUE), equals(TRUE))
  expect_that(isit("<", list(3,-4), FALSE), equals(TRUE))
  expect_that(isit(">", list(13,4), TRUE), equals(TRUE))
  expect_that(isit(">", list(4,4), FALSE), equals(TRUE))
  expect_that(isit("<=", list(3,4), TRUE), equals(TRUE))
  expect_that(isit("<=", list(3,-4), FALSE), equals(TRUE))
  expect_that(isit(">=", list(4,4), TRUE), equals(TRUE))
  expect_that(isit(">=", list(3,4), FALSE), equals(TRUE))
  expect_that(isit("==", list(3,3), TRUE), equals(TRUE))
  expect_that(isit("==", list(3,-4), FALSE), equals(TRUE))
  expect_that(isit("!=", list(3,4), TRUE), equals(TRUE))
  expect_that(isit("!=", list(-4,-4), FALSE), equals(TRUE))
  expect_that(isit("%in%", list(3,4), TRUE), equals(TRUE))
  expect_that(isit("%in%", list(3,-4), FALSE), equals(TRUE))
  expect_that(isit("!", list(FALSE), TRUE), equals(TRUE))
  expect_that(isit("!", list(TRUE), FALSE), equals(TRUE))
  expect_that(isit("|", list(FALSE, TRUE), TRUE), equals(TRUE))
  expect_that(isit("|", list(FALSE, FALSE), FALSE), equals(TRUE))
  expect_that(isit("&", list(TRUE, TRUE), TRUE), equals(TRUE))
  expect_that(isit("&", list(FALSE,TRUE), FALSE), equals(TRUE))
  expect_that(isit("min", list(3,4), 3), equals(TRUE))
  expect_that(isit("max", list(3,4), 4), equals(TRUE))
  expect_that(isit("length", list(c(3,4,5)), 3), equals(TRUE))
  expect_that(isit("is.na", list(NA), TRUE), equals(TRUE))
  expect_that(isit("sum", list(c(3,4,5)), 12), equals(TRUE))
